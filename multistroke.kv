#:kivy 1.11.1
<MainMenu>:
    rows: 1
    size_hint: (1, None)
    #height: 50
    spacing: 3
    padding: 3
    ToggleButton:
        id: btn1
        group: 'mainmenu'
        background_normal: "200x200.png"
        background_down: "reve.png"
        on_press:
            app.manager.current = 'screen1'
            if self.state == 'normal': self.state = 'down'
